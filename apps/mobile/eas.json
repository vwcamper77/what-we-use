{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "ios-testflight": {
      "distribution": "store",
      "channel": "production"
    },
    "android-internal": {
      "distribution": "store",
      "channel": "production"
    },
    "production": {
      "distribution": "store",
      "channel": "production"
    }
  },
  "submit": {
    "ios-testflight": {
      "ios": {}
    },
    "android-internal": {
      "android": {
        "track": "internal"
      }
    },
    "production": {}
  }
}
